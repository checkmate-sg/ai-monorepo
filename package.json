{
  "name": "checkmate-ai-monorepo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "wrangler dev -c workers/api-entrypoint/wrangler.jsonc -c workers/agent-service/wrangler.jsonc -c workers/database-service/wrangler.jsonc -c workers/embedder-service/wrangler.jsonc -c workers/screenshot-backup-service/wrangler.jsonc -c workers/search-service/wrangler.jsonc -c workers/trivialfilter-service/wrangler.jsonc -c workers/urlscan-service/wrangler.jsonc  -c workers/notification-service/wrangler.jsonc -c workers/blacklist-service/wrangler.jsonc",
    "dev:turbo": "turbo dev --filter=!./ --concurrency=12",
    "build": "turbo build --filter=!./",
    "lint": "turbo lint",
    "dev:search-service": "turbo dev --filter=search-service",
    "dev:urlscan-service": "turbo dev --filter=urlscan-service",
    "dev:screenshot-service": "turbo dev --filter=screenshot-service",
    "dev:screenshot-backup-service": "turbo dev --filter=screenshot-backup-service",
    "dev:trivialfilter-service": "turbo dev --filter=trivialfilter-service",
    "dev:agent-service": "turbo dev --filter=agent-service",
    "dev:api-entrypoint": "turbo dev --filter=api-entrypoint",
    "dev:embedder-service": "turbo dev --filter=embedder-service",
    "eval": "pnpm --filter evals run eval:local"
  },
  "workspaces": [
    "workers/*",
    "shared/*",
    "evals"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.5.2",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250525.0",
    "turbo": "^2.4.4",
    "typescript": "^5.8.2",
    "wrangler": "^4.40.3"
  },
  "dependencies": {
    "pino": "^9.6.0"
  },
  "private": true
}
